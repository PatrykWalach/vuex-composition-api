{
  "name": "vuex-composition-api",
  "version": "3.0.0",
  "description": "Make Vuex modules with syntax inspired by @vue/composition-api",
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-deno"
      ]
    ]
  },
  "scripts": {
    "build": "pika build",
    "coverage": "jest --collect-coverage",
    "codecov": "codecov",
    "lint": "eslint --ext .js,.jsx,.vue,.ts,.tsx --fix .",
    "test": "jest",
    "watch": "jest --watch"
  },
  "keywords": [
    "vue",
    "composition-api",
    "router",
    "hooks",
    "typescript"
  ],
  "author": "Patryk WaÅ‚ach",
  "license": "MIT",
  "peerDependencies": {
    "vue": ">=3.0.0-alpha.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PatrykWalach/vuex-composition-api.git"
  },
  "devDependencies": {
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-deno": "^0.9.2",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-build-web": "^0.9.2",
    "@pika/plugin-ts-standard-pkg": "^0.9.2",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.1.0",
    "@vue/test-utils": "^2.0.0-beta.5",
    "codecov": "^3.7.2",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.0.0-beta.3",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^9.5.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.0",
    "typescript": "^3.9.7",
    "vue": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": "eslint --cache --fix"
  }
}
